<h1>Question</h1>

<% @questions.each do |q| %>
  <h2><%= q.title %></h2>

  <time datetime="<%= q.formated_created_at %>"><%= q.formated_created_at %></time>

  <p><%= q.body %></p>
  <br>
  <%= link_to 'Show', question_path(q) %>
  <%= link_to 'Edit', edit_question_path(q) %>
  <%= link_to 'Destroy', question_path(q), data: { method: :delete, confirm: 'Are you sure?'} %>

<% end %>